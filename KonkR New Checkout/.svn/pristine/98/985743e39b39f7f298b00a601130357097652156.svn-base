                    <com.konkr.Utils.MyTextView
                        android:id="@+id/tvDateOfBirth"
                        style="@style/MyTextView"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:background="@null"
                        android:hint="@string/sp_date_of_birth"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:imeActionLabel="Next"
                        android:imeOptions="actionNext"
                        android:inputType="text"
                        android:paddingBottom="@dimen/_10sdp"
                        android:paddingRight="@dimen/_10sdp"
                        android:paddingTop="@dimen/_10sdp"
                        android:singleLine="true"
                        android:textColorHint="@color/hint_text_color"
                        android:textSize="@dimen/_15ssp"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/View1"
                        mytextview:txt_custom_font="@string/font_mavenpro_regular" />

                    <View
                        android:id="@+id/view4"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_1sdp"
                        android:background="@color/view_color"
                        app:layout_constraintLeft_toLeftOf="@+id/tvDateOfBirth"
                        app:layout_constraintRight_toRightOf="@+id/tvDateOfBirth"
                        app:layout_constraintTop_toBottomOf="@id/tvDateOfBirth" />

                    <com.konkr.Utils.MyTextView
                        android:id="@+id/tvGender"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:drawableRight="@drawable/down_arrow"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:padding="@dimen/_10sdp"
                        android:text="@string/sp_gender"
                        android:textColor="@color/hint_color"
                        android:textColorHint="@color/hint_text_color"
                        android:textSize="@dimen/_15ssp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/view4"
                        mytextview:txt_custom_font="@string/font_mavenpro_regular" />

                    <android.support.v7.widget.AppCompatSpinner
                        android:id="@+id/spinGender"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:background="@null"
                        android:visibility="invisible"
                        app:layout_constraintBottom_toBottomOf="@+id/tvGender"
                        app:layout_constraintLeft_toLeftOf="@+id/tvGender"
                        app:layout_constraintRight_toRightOf="@+id/tvGender"
                        app:layout_constraintTop_toTopOf="@+id/tvGender" />

                    <View
                        android:id="@+id/view5"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:background="@color/colorGray"
                        app:layout_constraintLeft_toLeftOf="@+id/tvGender"
                        app:layout_constraintRight_toRightOf="@+id/tvGender"
                        app:layout_constraintTop_toBottomOf="@id/tvGender" />

                    <mabbas007.tagsedittext.TagsEditText
                        android:id="@+id/etCurrentTrainingGoal"
                        style="@style/MyEditText"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@null"
                        android:hint="@string/sp_current_training_goal"
                        android:imeActionLabel="Next"
                        android:imeOptions="actionNext"
                        android:inputType="text"
                        android:padding="@dimen/_10sdp"
                        android:textColorHint="@color/hint_text_color"
                        android:textSize="@dimen/_15ssp"
                        TagsEditText:allowSpaceInTag="true"
                        TagsEditText:tagsBackground="@drawable/square"
                        TagsEditText:tagsCloseImagePadding="@dimen/defaultTagsCloseImagePadding"
                        TagsEditText:tagsCloseImageRight="@drawable/tag_close"
                        TagsEditText:tagsTextColor="@color/hint_color"
                        TagsEditText:tagsTextSize="@dimen/defaultTagsTextSize"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tvGender"
                        myedittext:edt_custom_font="@string/font_mavenpro_regular" />

                    <View
                        android:id="@+id/view25"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:background="@color/colorGray"
                        app:layout_constraintLeft_toLeftOf="@+id/etCurrentTrainingGoal"
                        app:layout_constraintRight_toRightOf="@+id/etCurrentTrainingGoal"
                        app:layout_constraintTop_toBottomOf="@id/etCurrentTrainingGoal" />

                    <com.konkr.Utils.MyTextView
                        android:id="@+id/tvBodyType"
                        style="@style/MyTextView"
                        android:layout_width="match_parent"
