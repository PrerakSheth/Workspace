package com.konkr.Models;

public class SendMediaPhotoComments {


    /**
     * message : You gave your comment on this media successfully.
     * status : 1
     * userCommentData : {"userId":4,"profilePic":"http://dev2.ifuturz.com/core/konkr/assets/upload/profile/4_thumb.png","firstName":"Tony","lastName":"Starck","badge":"1","comment":"lol"}
     */

    private String message;
    private int status;
    private UserCommentDataBean userCommentData;

    public String getMessage() {
        return message;
    }

    public void setMessage(String message) {
        this.message = message;
    }

    public int getStatus() {
        return status;
    }

    public void setStatus(int status) {
        this.status = status;
    }

    public UserCommentDataBean getUserCommentData() {
        return userCommentData;
    }

    public void setUserCommentData(UserCommentDataBean userCommentData) {
        this.userCommentData = userCommentData;
    }

    public static class UserCommentDataBean {
        /**
         * userId : 4
         * profilePic : http://dev2.ifuturz.com/core/konkr/assets/upload/profile/4_thumb.png
         * firstName : Tony
         * lastName : Starck
         * badge : 1
         * comment : lol
         */

        private int userId;
        private String profilePic;
        private String firstName;
        private String lastName;
        private String badge;
        private String comment;

        public int getUserId() {
            return userId;
        }

        public void setUserId(int userId) {
            this.userId = userId;
        }

        public String getProfilePic() {
            return profilePic;
        }

        public void setProfilePic(String profilePic) {
            this.profilePic = profilePic;
        }

        public String getFirstName() {
            return firstName;
        }

        public void setFirstName(String firstName) {
            this.firstName = firstName;
        }

        public String getLastName() {
            return lastName;
        }

        public void setLastName(String lastName) {
            this.lastName = lastName;
        }

        public String getBadge() {
            return badge;
