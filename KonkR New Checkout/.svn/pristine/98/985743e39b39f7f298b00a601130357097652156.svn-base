                        />

                    <ImageView
                        android:id="@+id/ivPhotoThree"
                        android:layout_width="@dimen/_50sdp"
                        android:layout_height="@dimen/_50sdp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:background="@drawable/mi_camera_placeholder"
                        android:clickable="false"
                        app:layout_constraintLeft_toRightOf="@+id/ivPhotoTwo"
                        app:layout_constraintRight_toLeftOf="@+id/ivPhotoFour"
                        app:placeholderImage="@drawable/mi_camera_placeholder"
                         />

                    <ImageView
                        android:id="@+id/ivPhotoFour"
                        android:layout_width="@dimen/_50sdp"
                        android:layout_height="@dimen/_50sdp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:background="@drawable/mi_camera_placeholder"
                        android:clickable="false"
                        app:layout_constraintLeft_toRightOf="@+id/ivPhotoThree"
                        app:layout_constraintRight_toLeftOf="@+id/ivPhotoFive"
                        app:placeholderImage="@drawable/mi_camera_placeholder"
                       />

                    <ImageView
                        android:id="@+id/ivPhotoFive"
                        android:layout_width="@dimen/_50sdp"
                        android:layout_height="@dimen/_50sdp"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginRight="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:background="@drawable/mi_camera_placeholder"
                        android:clickable="false"
                        app:layout_constraintLeft_toRightOf="@+id/ivPhotoFour"
                        app:layout_constraintRight_toRightOf="parent"
                        app:placeholderImage="@drawable/mi_camera_placeholder"

                      />

                    <ImageView
                        android:id="@+id/ivPhotoSix"
                        android:layout_width="@dimen/_50sdp"
                        android:layout_height="@dimen/_50sdp"
                        android:layout_marginLeft="@dimen/_20sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:background="@drawable/mi_camera_placeholder"
                        android:clickable="false"
                        app:layout_constraintLeft_toLeftOf="@+id/ivPhotoOne"
                        app:layout_constraintTop_toBottomOf="@+id/ivPhotoOne"
                        app:placeholderImage="@drawable/mi_camera_placeholder"
                      />

                    <ImageView
                        android:id="@+id/ivPhotoSeven"
                        android:layout_width="@dimen/_50sdp"
                        android:layout_height="@dimen/_50sdp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:background="@drawable/mi_camera_placeholder"
