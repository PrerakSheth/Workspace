public class FollowersList {


    /**
     * message : Followers get successfully.
     * status : 1
     * followersList : [{"userId":356,"profilePic":"http://dev2.ifuturz.com/core/konkr/assets/upload/profile/356_thumb.png","firstName":"Sames","lastName":"Piza","badge":"1","followUnfollow":0}]
     */

    private String message;
    private int status;
    private List<FollowersListBean> followersList;

    public String getMessage() {
        return message;
    }

    public void setMessage(String message) {
        this.message = message;
    }

    public int getStatus() {
        return status;
    }

    public void setStatus(int status) {
        this.status = status;
    }

    public List<FollowersListBean> getFollowersList() {
        return followersList;
    }

    public void setFollowersList(List<FollowersListBean> followersList) {
        this.followersList = followersList;
    }

    public static class FollowersListBean {
        /**
         * userId : 356
         * profilePic : http://dev2.ifuturz.com/core/konkr/assets/upload/profile/356_thumb.png
         * firstName : Sames
         * lastName : Piza
         * badge : 1
         * followUnfollow : 0
         */

        private int userId;
        private String profilePic;
        private String firstName;
        private String lastName;
        private String badge;
        private int followUnfollow;

        public int getUserId() {
            return userId;
        }

        public void setUserId(int userId) {
            this.userId = userId;
        }

        public String getProfilePic() {
            return profilePic;
        }

        public void setProfilePic(String profilePic) {
            this.profilePic = profilePic;
        }

        public String getFirstName() {
            return firstName;
        }

        public void setFirstName(String firstName) {
            this.firstName = firstName;
        }

        public String getLastName() {
            return lastName;
        }

        public void setLastName(String lastName) {
            this.lastName = lastName;
        }

        public String getBadge() {
            return badge;
        }

        public void setBadge(String badge) {
            this.badge = badge;
        }

        public int getFollowUnfollow() {
            return followUnfollow;
        }

        public void setFollowUnfollow(int followUnfollow) {
            this.followUnfollow = followUnfollow;
        }
    }
}
