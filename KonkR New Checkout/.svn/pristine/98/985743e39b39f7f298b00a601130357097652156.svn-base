                        android:clickable="false"
                        app:layout_constraintLeft_toRightOf="@+id/ivPhotoSeven"
                        app:layout_constraintTop_toBottomOf="@+id/ivPhotoOne"
                        app:placeholderImage="@drawable/mi_camera_placeholder"
                        />

                    <ImageView
                        android:id="@+id/ivPhotoNine"
                        android:layout_width="@dimen/_50sdp"
                        android:layout_height="@dimen/_50sdp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:background="@drawable/mi_camera_placeholder"
                        android:clickable="false"
                        app:layout_constraintLeft_toRightOf="@+id/ivPhotoEight"
                        app:layout_constraintTop_toBottomOf="@+id/ivPhotoOne"
                        app:placeholderImage="@drawable/mi_camera_placeholder"
                        />
                </android.support.constraint.ConstraintLayout>

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/clAddIntroductoryVideo"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/scale_90dp"
                    android:background="@drawable/add_iv_shadow"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    app:layout_constraintBottom_toTopOf="@+id/clInnerData"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/clNineImage">

                    <com.konkr.Utils.MyTextView
                        android:id="@+id/tvAddIntroductoryVideo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:drawableRight="@drawable/left_arrow"
                        android:gravity="center_vertical"
                        android:text="@string/sp_add_introductory_video"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_16ssp"
                        mytextview:txt_custom_font="@string/font_mavenpro_medium" />

                    <android.support.constraint.ConstraintLayout
                        android:id="@+id/clVideoView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:visibility="gone">

                        <com.facebook.drawee.view.SimpleDraweeView
                            android:id="@+id/ivIntroductoryVideo"
                            android:layout_width="@dimen/_80sdp"
                            android:layout_height="match_parent"
                            android:background="@drawable/placeholder"
                            app:actualImageScaleType="focusCrop"
                            app:fadeDuration="200"
                            app:failureImageScaleType="centerInside"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:placeholderImageScaleType="fitCenter"
                            app:retryImageScaleType="centerCrop"
                            app:roundAsCircle="false" />

                        <ImageButton
                            android:id="@+id/ibPlay"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:background="@null"
                            android:clickable="false"
                            android:src="@drawable/play"
                            app:layout_constraintBottom_toBottomOf="@+id/ivIntroductoryVideo"
                            app:layout_constraintLeft_toLeftOf="@+id/ivIntroductoryVideo"
                            app:layout_constraintRight_toRightOf="@+id/ivIntroductoryVideo"
                            app:layout_constraintTop_toTopOf="@+id/ivIntroductoryVideo" />

                        <com.konkr.Utils.MyTextView
                            android:id="@+id/tvAboutMe"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:gravity="center"
                            android:text="@string/sp_add_introductory_video_about_me"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_16ssp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toRightOf="@+id/ivIntroductoryVideo"
                            app:layout_constraintRight_toLeftOf="@+id/ibEdit"
                            app:layout_constraintTop_toTopOf="parent"
                            mytextview:txt_custom_font="@string/font_mavenpro_medium" />

                        <ImageButton
                            android:id="@+id/ibEdit"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:background="@null"
                            android:padding="@dimen/_10sdp"
                            android:src="@drawable/edit_black"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />
                    </android.support.constraint.ConstraintLayout>
                </android.support.constraint.ConstraintLayout>

                <android.support.constraint.ConstraintLayout
                    android:id="@+id/clInnerData"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:background="@drawable/add_iv_shadow"
                    app:layout_constraintBottom_toTopOf="@+id/clUpdateProfile"
                    app:layout_constraintTop_toBottomOf="@+id/clAddIntroductoryVideo">

                    <android.support.constraint.Guideline
                        android:id="@+id/glData"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        TagsEditText:layout_constraintGuide_begin="180dp"
                        app:layout_constraintGuide_percent=".5" />

                    <com.konkr.Utils.MyEditText
                        android:id="@+id/etFirstName"
                        style="@style/MyEditText"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:background="@null"
                        android:hint="@string/sp_first_name"
                        android:imeActionLabel="Next"
                        android:imeOptions="actionNext"
                        android:inputType="textCapSentences"
                        android:maxLength="25"
                        android:padding="@dimen/_10sdp"
                        android:singleLine="true"
                        android:textColorHint="@color/hint_text_color"
                        android:textSize="@dimen/_15ssp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toLeftOf="@+id/glData"
                        app:layout_constraintTop_toTopOf="parent"
                        myedittext:edt_custom_font="@string/font_mavenpro_regular" />

                    <View
                        android:id="@+id/View1"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_1sdp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_10sdp"
                        android:background="@color/colorGray"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toLeftOf="@+id/glData"
                        app:layout_constraintTop_toBottomOf="@+id/etFirstName" />

                    <com.konkr.Utils.MyEditText
                        android:id="@+id/etLastName"
                        style="@style/MyEditText"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:background="@null"
                        android:hint="@string/sp_last_name"
                        android:imeActionLabel="Next"
                        android:imeOptions="actionNext"
                        android:inputType="textCapSentences"
                        android:maxLength="25"
                        android:paddingBottom="@dimen/_10sdp"
                        android:paddingRight="@dimen/_10sdp"
                        android:paddingTop="@dimen/_10sdp"
                        android:singleLine="true"
                        android:textColorHint="@color/hint_text_color"
                        android:textSize="@dimen/_15ssp"
                        app:layout_constraintBottom_toBottomOf="@+id/etFirstName"
                        app:layout_constraintLeft_toRightOf="@+id/glData"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/etFirstName"
                        myedittext:edt_custom_font="@string/font_mavenpro_regular" />

                    <View
                        android:id="@+id/view2"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_1sdp"
                        android:background="@color/view_color"
                        app:layout_constraintLeft_toLeftOf="@+id/etLastName"
                        app:layout_constraintRight_toRightOf="@+id/etLastName"
                        app:layout_constraintTop_toBottomOf="@id/etLastName" />

                    <!--<com.konkr.Utils.MyEditText-->
                        <!--android:id="@+id/etNickname"-->
                        <!--style="@style/MyEditText"-->
                        <!--android:layout_width="0dp"-->
